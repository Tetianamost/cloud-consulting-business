{
  "environment": "ci",
  "database": {
    "driver": "postgres",
    "url": "postgres://postgres:postgres@localhost:5432/test_ci_db?sslmode=disable",
    "max_connections": 20,
    "conn_max_lifetime": "30m",
    "conn_max_idle_time": "5m",
    "migrations_path": "backend/scripts",
    "seed_data_path": "backend/testing/fixtures"
  },
  "redis": {
    "url": "redis://localhost:6379/5",
    "password": "",
    "db": 5,
    "max_retries": 3,
    "dial_timeout": "5s",
    "read_timeout": "3s"
  },
  "aws": {
    "region": "us-east-1",
    "access_key_id": "",
    "secret_access_key": "",
    "session_token": "",
    "use_mocks": true
  },
  "bedrock": {
    "base_url": "http://localhost:8080",
    "model": "anthropic.claude-3-sonnet-20240229-v1:0",
    "max_tokens": 1000,
    "temperature": 0.7,
    "timeout": "20s",
    "use_mock": true
  },
  "ses": {
    "sender_email": "test@example.com",
    "reply_to_email": "test@example.com",
    "timeout": "10s",
    "use_mock": true,
    "verified_emails": ["test@example.com"]
  },
  "logging": {
    "level": "error",
    "format": "json",
    "output": "stdout",
    "structured": true
  },
  "performance": {
    "max_response_time": "10s",
    "max_memory_usage": 200000000,
    "max_cpu_usage": 80.0,
    "concurrent_requests": 20,
    "test_duration": "1m",
    "warmup_duration": "5s"
  },
  "quality": {
    "min_accuracy": 0.75,
    "min_completeness": 0.70,
    "min_relevance": 0.75,
    "min_actionability": 0.70,
    "min_technical_depth": 0.65,
    "min_business_value": 0.70
  },
  "timeout": "1m",
  "parallel": true,
  "verbose": false,
  "skip_slow": true,
  "cleanup_db": true,
  "seed_data": false,
  "custom_vars": {
    "ci_build_id": "",
    "ci_commit_sha": "",
    "ci_branch": ""
  }
}