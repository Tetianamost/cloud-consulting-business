  "quality": {
    "min_accuracy": 0.70,
    "min_completeness": 0.65,
    "min_relevance": 0.70,
    "min_actionability": 0.65,
    "min_technical_depth": 0.60,
    "min_business_value": 0.65
  },
  "timeout": "10s",
  "parallel": true,
  "verbose": false,
  "skip_slow": true,
  "cleanup_db": false,
  "seed_data": false,
  "custom_vars": {}
