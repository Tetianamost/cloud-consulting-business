# Basic configuration for local development
PORT=8061
LOG_LEVEL=4
GIN_MODE=debug
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# Amazon Bedrock configuration
# Get your token from AWS Console -> Bedrock -> Model access
AWS_BEARER_TOKEN_BEDROCK=your_bedrock_token_here
BEDROCK_REGION=us-east-1
BEDROCK_MODEL_ID=amazon.nova-lite-v1:0
BEDROCK_BASE_URL=https://bedrock-runtime.us-east-1.amazonaws.com
BEDROCK_TIMEOUT_SECONDS=30

# AWS SES configuration
# Get your credentials from AWS Console -> IAM -> Users
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_SES_REGION=us-east-1
SES_SENDER_EMAIL=info@cloudpartner.pro
SES_REPLY_TO_EMAIL=info@cloudpartner.pro
SES_TIMEOUT_SECONDS=30