# AWS Credentials for Bedrock Integration
# IMPORTANT: Never commit real credentials to version control
# Copy this file to .env and fill in your actual values

# AWS Access Keys (if not using IAM roles)
AWS_ACCESS_KEY_ID=access
AWS_SECRET_ACCESS_KEY=secret

# AWS Region
AWS_REGION=us-east-1

# Amazon Bedrock Configuration
AWS_BEARER_TOKEN_BEDROCK=bedrock
BEDROCK_REGION=us-east-1
BEDROCK_MODEL_ID=amazon.nova-lite-v1:0
BEDROCK_BASE_URL=https://bedrock-runtime.us-east-1.amazonaws.com
BEDROCK_TIMEOUT_SECONDS=30

# Application Configuration
PORT=8061
LOG_LEVEL=4
GIN_MODE=debug

# Frontend Configuration
REACT_APP_API_URL=http://localhost:8061
REACT_APP_WS_URL=ws://localhost:8061/api/v1/admin/chat/ws